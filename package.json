{
    "name": "promise-pool-executor",
    "version": "1.1.1",
    "description": "A module for managing ES6 promise concurrency, frequency, and efficiency.",
    "keywords": [
        "promise",
        "promises",
        "concurrency",
        "frequency",
        "typescript",
        "throttle",
        "batching",
        "es6",
        "async",
        "await",
        "pool",
        "queue",
        "limit"
    ],
    "author": "Wes van Vugt <wes.vanvugt@gmail.com>",
    "repository": "WesVanVugt/promise-pool-executor",
    "scripts": {
        "prepare": "npm run build",
        "build": "npm run clean && npm run tsc",
        "clean": "rm -rf index.js index.d.ts private public test",
        "test": "npm run lint && npm run test:local",
        "test:local": "nyc mocha --colors --require ts-node/register ts/test/*.test.ts",
        "test-debug": "cross-env DEBUG=promise-pool-executor:*,-mocha:* npm test",
        "tsc": "tsc",
        "lint": "tslint ts/**/*.ts"
    },
    "main": "index.js",
    "typings": "index.d.ts",
    "engines": {
        "node": ">=5.0.0"
    },
    "files": [
        "index.js",
        "index.d.ts",
        "private/*.js",
        "public/*"
    ],
    "dependencies": {
        "p-defer": "^3.0.0",
        "promise-batcher": "^1.1.0"
    },
    "devDependencies": {
        "@sinonjs/fake-timers": "^7.1.2",
        "@types/chai": "^4.2.21",
        "@types/chai-as-promised": "^7.1.4",
        "@types/mocha": "^8.2.3",
        "@types/node": "^14.17.5",
        "@types/sinonjs__fake-timers": "^6.0.3",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "cross-env": "^7.0.3",
        "mocha": "^9.0.2",
        "nyc": "^15.1.0",
        "time-span": "^4.0.0",
        "ts-node": "^10.1.0",
        "tslint": "^5.12.1",
        "typescript": "^4.3.5"
    },
    "sideEffects": false,
    "license": "MIT"
}
