{
    "name": "promise-pool-executor",
    "version": "1.1.1",
    "description": "A module for managing ES6 promise concurrency, frequency, and efficiency.",
    "keywords": [
        "promise",
        "promises",
        "concurrency",
        "frequency",
        "typescript",
        "throttle",
        "batching",
        "es6",
        "async",
        "await",
        "pool",
        "queue",
        "limit"
    ],
    "author": "Wes van Vugt <wes.vanvugt@gmail.com>",
    "repository": "WesVanVugt/promise-pool-executor",
    "scripts": {
        "prepare": "npm run build",
        "build": "npm run clean && npm run lint && npm run tsc",
        "clean": "rm -rf index.js index.d.ts private public test",
        "test": "mocha test/*.js --reporter spec --colors --check-leaks",
        "test-debug": "cross-env DEBUG=*,-mocha:* npm test",
        "tsc": "tsc",
        "lint": "tslint ts/**/*.ts"
    },
    "main": "index.js",
    "typings": "index.d.ts",
    "engines": {
        "node": ">=5.0.0"
    },
    "files": [
        "index.js",
        "index.d.ts",
        "private/*.js",
        "public/*"
    ],
    "dependencies": {
        "debug": "^4.1.1",
        "next-tick": "^1.0.0",
        "p-defer": "^1.0.0",
        "promise-batcher": "^1.0.2"
    },
    "devDependencies": {
        "@types/chai-as-promised": "^7.1.0",
        "@types/debug": "^4.1.1",
        "@types/mocha": "^5.2.6",
        "@types/next-tick": "^1.0.0",
        "@types/node": "^11.9.4",
        "@types/p-defer": "^1.0.3",
        "@types/time-span": "^2.0.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "cross-env": "^5.2.0",
        "mocha": "^5.2.0",
        "time-span": "^2.0.0",
        "tslint": "^5.12.1",
        "typescript": "^3.3.3"
    },
    "license": "MIT",
    "public": true
}
