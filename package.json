{
	"name": "promise-pool-executor",
	"version": "1.1.1",
	"description": "A module for managing ES6 promise concurrency, frequency, and efficiency.",
	"keywords": [
		"promise",
		"promises",
		"concurrency",
		"frequency",
		"typescript",
		"throttle",
		"batching",
		"es6",
		"async",
		"await",
		"pool",
		"queue",
		"limit"
	],
	"author": "Wes van Vugt <wes.vanvugt@gmail.com>",
	"repository": "WesVanVugt/promise-batcher",
	"scripts": {
		"prepare": "npm run build",
		"build": "npm run clean && tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.nocomments.json && rm private/*.d.ts && prettier -w index.js index.d.ts private public",
		"clean": "rm -rf index.js index.d.ts private public",
		"test": "npm run lint && npm run test:local",
		"test:local": "jest",
		"test:debug": "cross-env NODE_DEBUG=promise-pool-executor* npm test",
		"lint": "eslint --max-warnings 0"
	},
	"main": "index.js",
	"typings": "index.d.ts",
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"p-defer": "^3.0.0",
		"promise-batcher": "^1.1.1"
	},
	"devDependencies": {
		"@tsconfig/node18": "^18.2.4",
		"@types/eslint": "^9.6.1",
		"@types/jest": "^30.0.0",
		"@types/node": "^18.19.112",
		"@typescript-eslint/eslint-plugin": "^8.34.1",
		"@typescript-eslint/parser": "^8.34.1",
		"cross-env": "^7.0.3",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-jest": "^29.0.1",
		"eslint-plugin-prettier": "^5.5.0",
		"jest": "^30.0.2",
		"jest-fail-on-console": "^3.3.1",
		"mimic-fn": "^3.1.0",
		"prettier": "^3.6.0",
		"time-span": "^4.0.0",
		"ts-expect": "^1.3.0",
		"ts-jest": "^29.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.1"
	},
	"sideEffects": false,
	"license": "MIT"
}