{
	"name": "promise-pool-executor",
	"version": "1.1.1",
	"description": "A module for managing ES6 promise concurrency, frequency, and efficiency.",
	"keywords": [
		"promise",
		"promises",
		"concurrency",
		"frequency",
		"typescript",
		"throttle",
		"batching",
		"es6",
		"async",
		"await",
		"pool",
		"queue",
		"limit"
	],
	"author": "Wes van Vugt <wes.vanvugt@gmail.com>",
	"repository": "WesVanVugt/promise-batcher",
	"scripts": {
		"prepare": "npm run build",
		"build": "npm run clean && npm run tsc && prettier -w index.js index.d.ts private public",
		"clean": "rm -rf index.js index.d.ts private public test",
		"test": "mocha test/*.js --reporter spec --colors --check-leaks",
		"test-debug": "cross-env DEBUG=*,-mocha:* npm test",
		"tsc": "tsc",
		"lint": "eslint --color --max-warnings 0 --ext .ts ."
	},
	"main": "index.js",
	"typings": "index.d.ts",
	"engines": {
		"node": ">=5.0.0"
	},
	"dependencies": {
		"debug": "^3.1.0",
		"next-tick": "^1.0.0",
		"p-defer": "^1.0.0",
		"promise-batcher": "^1.0.1"
	},
	"devDependencies": {
		"@types/chai-as-promised": "^7.1.0",
		"@types/debug": "^0.0.30",
		"@types/mocha": "^5.2.0",
		"@types/node": "^9.6.6",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"cross-env": "^5.1.4",
		"eslint": "^8.33.0",
		"eslint-plugin-prettier": "^4.2.1",
		"mocha": "^5.1.1",
		"prettier": "^2.8.3",
		"typescript": "^4.9.4"
	},
	"sideEffects": false,
	"license": "MIT"
}