{
	"compileOnSave": true,
	"compilerOptions": {
		"newLine": "lf",
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"preserveConstEnums": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"esModuleInterop": true,
		"declaration": true,
		"sourceMap": false,
		"outDir": "."
	},
	"include": [
		"**/*"
	],
	"exclude": []
}