{
	"compileOnSave": true,
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"preserveConstEnums": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"declaration": true,
		"sourceMap": false,
		"outDir": "."
	},
	"include": [
		"ts/index.ts",
		"ts/test/index.ts",
		"ts/typings/**/*"
	],
	"exclude": []
}